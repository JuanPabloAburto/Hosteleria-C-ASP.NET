@model Negocio.Admin.ListEmpresa.ListEmpresa

@{
    ViewBag.Title = "Detalle_Empresa";
    Layout = "~/Views/Shared/Layout_Usuarios/_Layout_Home_Admin.cshtml";
}

<div class="container">
    <h2>
        <strong>
            Detalle Empresa clientes
        </strong>
    </h2>
    <hr />

    <div class="container row">
        <div class="col-md-6">
            <h5>
                <strong>
                    Nombre
                </strong>
            </h5>
            <h6>
                @Html.DisplayFor(model => model.NOMBRE_E)
            </h6>
        </div>

        <div class="col-md-6">
            <h5>
                <strong>
                    Razon social
                </strong>
            </h5>
            <h6>
                @Html.DisplayFor(model => model.RAZONSOCIAL_E)
            </h6>
        </div>

        <div class="col-md-6">
            <h5>
                <strong>
                   Region
                </strong>
            </h5>
            <h6>
                @Html.DisplayFor(model => model.REGION)
            </h6>
        </div>

        <div class="col-md-6">
            <h5>
                <strong>
                    Telefono
                </strong>
            </h5>
            <h6>
                @Html.DisplayFor(model => model.TELEFONO_E)
            </h6>
        </div>



    </div>
    <hr />


</div>
<div class="container row">
    <div class="col-md-3">
        @Html.ActionLink("Editar", "Editar_Empresa", new { id = Model.RUT_E }, new { @class = "btn btn-warning" })
        <a class="btn btn-secondary mx-2" href="/admin/listEmpresa">Volver</a>

    </div>

</div>

