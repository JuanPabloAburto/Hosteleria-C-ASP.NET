<!DOCTYPE html>
<html lang="es-cl">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Hostal Doña Clarita</title>

    <!-- Custom fonts for this template-->
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">


    <!-- Custom styles for this template-->
    @Styles.Render("~/Content/Home/css/sb-admin-2.min.css")
    @Styles.Render("~/Content/Home/css/Custom.css")



    @Styles.Render("~/Content/EstiloTabla/datatables/datatables.min.css")
    @Styles.Render("~/Content/EstiloTabla/datatables/DataTables-1.10.18/css/dataTables.bootstrap4.min.css")


</head>


<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">


        <!-- Sidebar -->
        <ul class="navbar-nav  sidebar sidebar-dark accordion ColorAdministrador " id="accordionSidebar">


            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center">
                <div class="sidebar-brand-icon rotate-n-15">
                    <i class="fas fa-bed"></i>
                </div>
                <div class="sidebar-brand-text mx-3">Administrador</div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Home -->
            <li class="nav-item active">
                <a class="nav-link" href="/Admin/Home_Admin">
                    <i class="fas fa-fw fa-home"></i>
                    <span class="efectoAmarillo">Home<sup>Administrador</sup></span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <hr class="sidebar-divider d-none d-md-block">

            <!-- Heading -->
            <div class="sidebar-heading">
                Funciones
            </div>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/Listado_Habitacion">
                    <i class="fas fa-fw fa-th-list"></i>
                    <span class="efectoAmarillo">Listado de Habitaciones</span>
                </a>
            </li>


            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Proveedores
            </div>



            <li class="nav-item">
                <a class="nav-link" href="/admin/listProveedores">
                    <i class="fas fa-fw fa-list"></i>
                    <span class="efectoAmarillo">Lista de Proveedores</span>
                </a>
                <a href="#" class="nav-link" data-toggle="modal" data-target="#Proveedor">
                    <i class="fas fa-fw fa-list"></i>
                    <span class="efectoAmarillo">Insertar Proveedor</span>
                </a>
            </li>





            <!-- Modal -->
            <div class="modal fade" id="Proveedor" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <form action=@Url.Content("~/admin/Crear_Proveedor") method="get">
                        <div class="modal-content">

                            <div class="modal-header">

                                <h5 class="modal-title" id="exampleModalLabel">Ingrese el rut del proveedor</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <label>Rut</label>
                                <input type="text" name="id" id="exampleInputEmail1" class="form-control" required placeholder="Rut a Validar">
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                                <button type="submit" class="btn btn-warning">Validar</button>

                            </div>

                        </div>
                    </form>
                </div>
            </div>




            <hr class="sidebar-divider d-none d-md-block">

            <!-- Heading -->
            <div class="sidebar-heading">
                Empleados
            </div>

            <li class="nav-item">
                <a class="nav-link" href="/admin/listEmpleado">
                    <i class="fas fa-fw fa-user-tie" "></i>
                    <span class="efectoAmarillo">Lista de Empleados</span>
                </a>

                <a href="#" class="nav-link" data-toggle="modal" data-target="#exampleModal">
                    <i class="fas fa-fw fa-user-tie" "></i>
                    <span class="efectoAmarillo">Insertar Empleado</span>
                </a>

            </li>

            <!-- Heading -->
            <div class="sidebar-heading">
                Empresas
            </div>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/listEmpresa">
                    <i class="fas fa-fw fa-business-time"></i>
                    <span class="efectoAmarillo">Lista de Empresas</span>
                </a>

                <a class="nav-link" href="/Admin/Crear_Empresa">
                    <i class="fas fa-fw fa-business-time"></i>

                    <span class="efectoAmarillo">Insertar Empresa</span>
                </a>


            </li>

            <!-- Modal -->
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <form action=@Url.Content("~/admin/Crear_Empleado") method="get">
                        <div class="modal-content">

                            <div class="modal-header">

                                <h5 class="modal-title" id="exampleModalLabel">Ingrese Rut del empleado</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <label>Rut</label>
                                <input type="text" name="id" id="" class="form-control" required placeholder="Rut a Validar">
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                                <button type="submit" class="btn btn-warning">Validar</button>

                            </div>

                        </div>
                    </form>
                </div>
            </div>


            <!-- Heading -->
            <div class="sidebar-heading">
                Graficos
            </div>

            <li class="nav-item">
                <a class="nav-link" href="/Grafico/Visualizar">
                    <i class="fas fa-fw fa-business-time"></i>
                    <span class="efectoAmarillo">Minutas semanal</span>
                </a>

            </li>


            <div class="sidebar-heading">
                Cuenta
            </div>

            <li class="nav-item">

                <a class="nav-link" href="/Cerrar/CerrarSesion">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span class="efectoAmarillo">Cerrar Sesión</span>


                </a>

            </li>


            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>

        </ul>


        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">


                <br />
                <br />
                @RenderBody()

            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Hostal Doña Clarita 2020</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>









</body>








